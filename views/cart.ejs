<%- include('partials/header'); -%>
    </head>
    <body>
         <h1> Shopping Cart </h1><br>
         <p>Must change quantity in Pokemon lookup page</p>
    <form action = /logout>
<button> Logout </button>
</form>
<table>
  <tr>
    <th>Item</th>
    <th>Quantity</th>
    <th>Price</th>
  </tr>
  <%items.forEach(function(item){%>
  <tr>
    <td><%= item[0]%></td>
    <td><%= item[2]%></td>
    <td>$<%= item[1]%></td>
  </tr>
  <%})%>
</table>
<br>
<table>
  <tr>
    <th>Total: $</th>
    <td><%= total%></td>
  </tr>
</table>
<br><br>
<form action = /checkout>
<button> Cick here to purchase! </button>
</form>

</body>
<%- include('partials/footer'); -%>